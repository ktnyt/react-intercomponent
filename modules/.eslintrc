{
  "parser": "babel-eslint",
  "plugins": [
    "import",
    "react"
  ],
  "env": {
    "browser": true,
    "node": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:import/errors",
    "plugin:react/recommended"
  ],
  "rules": {
    "no-unused-vars": [ "error", { "args": "none" } ],
    "prefer-arrow-callback": 2,
    "semi": [ 2, "never" ]
  },
  "globals": {
    "__DEV__": true
  }
}
